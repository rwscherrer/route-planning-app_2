
<div>
<% @final_order = @final_order.sort_by { |i| i["row_order"]} %>
    <%  @final_order.each do |job| %>
  
        <h2><%= job['name'] %> </h2>
        <h3> <%= job['address'] %> </h3>
        <h4> <b> Distance:</b> <%= job['distance'].round(2) %> Priority: <%= job['priority'] %> <b> In Progress </b><%= job['in_progress'] %> </h4>
        <h4> <b>Conditions: </b><%= job['forecast'] %> <b>Temp (High/Low): </b><%= job['high'] %> / <%= job['low'] %></h4>
    <% end %>

</div>